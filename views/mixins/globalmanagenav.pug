mixin globalmanagenav(selected, upLevel)
	nav.pages
		if permissions.get(Permissions.MANAGE_GENERAL)
			a(href=`${upLevel ? '../' : ''}recent.html` class=(selected === 'recent' ? 'bold' : '')) [#{__('Recent')}]
			|
			|
			a(href=`${upLevel ? '../' : ''}reports.html` class=(selected === 'reports' ? 'bold' : '')) [#{__('Reports')}]
		if permissions.get(Permissions.MANAGE_FILE_APPROVAL)
			|
			|
			a(href=`/globalmanage/approval.html` class=(selected === 'approvals' ? 'bold' : '')) [#{__('Approvals')}]
		if permissions.get(Permissions.MANAGE_BANS)
			|  
			a(href=`${upLevel ? '../' : ''}bans.html` class=(selected === 'bans' ? 'bold' : '')) [#{__('Bans')}]
		if permissions.get(Permissions.MANAGE_BOARDS)
			|  
			a(href=`${upLevel ? '../' : ''}boards.html` class=(selected === 'boards' ? 'bold' : '')) [#{__('Boards')}]
		if permissions.get(Permissions.MANAGE_LOGS)
			|  
			a(href=`${upLevel ? '../' : ''}globallogs.html` class=(selected === 'logs' ? 'bold' : '')) [#{__('Logs')}]
		if permissions.get(Permissions.MANAGE_ACCOUNTS)
			|  
			a(href=`${upLevel ? '../' : ''}accounts.html` class=(selected === 'accounts' ? 'bold' : '')) [#{__('Accounts')}]
		if permissions.get(Permissions.MANAGE_ROLES)
			|  
			a(href=`${upLevel ? '../' : ''}roles.html` class=(selected === 'roles' ? 'bold' : '')) [#{__('Roles')}]
		if permissions.get(Permissions.MANAGE_NEWS)
			|  
			a(href=`${upLevel ? '../' : ''}news.html` class=(selected === 'news' ? 'bold' : '')) [#{__('News')}]
		if permissions.get(Permissions.MANAGE_GLOBAL_SETTINGS)
			|  
			a(href=`${upLevel ? '../' : ''}filters.html` class=(selected === 'filters' ? 'bold' : '')) [#{__('Filters')}]
		if permissions.get(Permissions.MANAGE_GLOBAL_SETTINGS)
			|  
			a(href=`${upLevel ? '../' : ''}settings.html` class=(selected === 'settings' ? 'bold' : '')) [#{__('Settings')}]
		if permissions.get(Permissions.MANAGE_ASSETS)
			|
			|
			a(href=`${upLevel ? '../' : ''}assets.html` class=(selected === 'assets' ? 'bold' : '')) [#{__('Assets')}]
		if permissions.get(Permissions.MANAGE_ASSETS)
			|
			|
			a(href=`${upLevel ? '../' : ''}custompages.html` class=(selected === 'custompages' ? 'bold' : '')) [#{__('Custome Pages')}]
		if permissions.get(Permissions.MANAGE_TRUSTED)
			|  
			a(href=`${upLevel ? '../' : ''}trusted.html` class=(selected === 'trusted' ? 'bold' : '')) [#{__('Trusted')}]


